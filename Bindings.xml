		if ( keystate == "up" ) then
			INFINITYSEARCH.show('');
		end
	</Binding>
	<Binding name="CLICK InfinitySearchOption1:LeftButton" runOnUp="true" category="InfinitySearch"/>
	<Binding name="CLICK InfinitySearchOption2:LeftButton" runOnUp="true" category="InfinitySearch"/>
	<Binding name="CLICK InfinitySearchOption3:LeftButton" runOnUp="true" category="InfinitySearch"/>
	<Binding name="CLICK InfinitySearchOption4:LeftButton" runOnUp="true" category="InfinitySearch"/>
	<Binding name="CLICK InfinitySearchOption5:LeftButton" runOnUp="true" category="InfinitySearch"/>
</Bindings>
